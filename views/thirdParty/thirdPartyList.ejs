<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>第三方信息库</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <%- include ../publicPages/css_content.ejs%>
    <link rel='stylesheet' href='/stylesheets/thirdParty/thirdPartyList.css' >
</head>
<body>
<!--头部区域-->
<%- include ../publicPages/header.ejs %>
<!--主体区域-->
<div id="main-body">
    <!--导航栏-->
    <%- include ../publicPages/nav.ejs %>
    <!--文本区域-->
    <div id="content-wrapper">
        <div class="breadcrumb-wrapper">
            <span class="layui-breadcrumb"><a href="javascript:void(0)"><cite>易格管理端</cite></a><a href="javascript:void(0)"><cite>第三方信息库</cite></a></span>
        </div>
        <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
            <legend>筛选条件</legend>
        </fieldset>
        <!--筛选条件-->
        <form class="filter-thirdParty layui-form">
            <input type="hidden" name="pageNo" value="1"/>
            <div class="layui-form-item">
                <label class="layui-form-label">第三方名称</label>
                <div class="layui-input-block">
                    <input type="text" name="thirdPartyName" autocomplete="off" placeholder="请输入第三方名称" class="layui-input">
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">所在地点</label>
                <div class="layui-input-inline">
                    <select name="provinceId" lay-filter="province">
                        <option value="">请选择省</option>
                    </select>
                </div>
                <div class="layui-input-inline">
                    <select name="cityId" lay-filter="city">
                        <option value="">请选择市</option>
                    </select>
                </div>
                <div class="layui-input-inline">
                    <select name="countryId">
                        <option value="">请选择县/区</option>
                    </select>
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">创建日期</label>
                <div class="layui-input-inline">
                    <input type="text" name="startTime" lay-verify="date" placeholder="yyyy-mm-dd" autocomplete="off" class="layui-input" onclick="layui.laydate({elem: this})">
                </div>
                <div class="layui-form-mid">-</div>
                <div class="layui-input-inline">
                    <input type="text" name="endTime"  lay-verify="date" placeholder="yyyy-mm-dd" autocomplete="off" class="layui-input" onclick="layui.laydate({elem: this})">
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">第三方性质</label>
                <div class="layui-input-block">
                    <input type="radio" name="type" value="" title="不限" checked="">
                    <input type="radio" name="type" value="1" title="学校">
                    <input type="radio" name="type" value="2" title="政府">
                </div>
            </div>
            <div class="layui-form-item">
                <div class="layui-input-block">
                    <button id="search" type="button" class="layui-btn">搜索</button>
                    <button type="reset" class="layui-btn layui-btn-primary">重置</button>
                </div>
            </div>
        </form>
        <!--第三方列表-->
        <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
            <legend>第三方列表</legend>
        </fieldset>
        <div class="thirdPartyTable-wrapper">
            <table id="thirdPartyList" class="layui-table" lay-even="" lay-skin="row">
                <thead>
                <tr>
                    <th>编号</th>
                    <th>第三方名称</th>
                    <th>类型</th>
                    <th>所在地区</th>
                    <th>详细地址</th>
                    <th>创建时间</th>
                    <th>官网URL</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
        <div id="page-wrapper"></div>
    </div>
</div>
</body>
<!--JS-->
<%- include ../publicPages/script_content.ejs %>
<script src="/javascripts/thirdParty/thirdPartyList.js"></script>
</html>
