<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>简历信息库</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <%- include ../publicPages/css_content.ejs%>
    <link rel='stylesheet' href='/stylesheets/resume/resumeList.css' >
</head>
<body>
<!--头部区域-->
<%- include ../publicPages/header.ejs %>
<!--主体区域-->
<div id="main-body">
    <!--导航栏-->
    <%- include ../publicPages/nav.ejs %>
    <!--文本区域-->
    <div id="content-wrapper">
        <div class="breadcrumb-wrapper">
            <span class="layui-breadcrumb"><a href="javascript:void(0)"><cite>易格管理端</cite></a><a href="javascript:void(0)"><cite>简历信息库</cite></a></span>
        </div>
        <!--筛选条件-->
        <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
            <legend>筛选条件</legend>
        </fieldset>
        <form class="filter-wrapper hide layui-form">
            <div class="layui-form-item">
                <label class="layui-form-label">姓名</label>
                <div class="layui-input-block">
                    <input type="text" autocomplete="off" placeholder="请输入姓名" class="layui-input">
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">工作地点</label>
                <div class="layui-input-inline">
                    <select>
                        <option value="">请选择省</option>
                        <option value="浙江" selected="">浙江省</option>
                        <option value="你的工号">江西省</option>
                        <option value="你最喜欢的老师">福建省</option>
                    </select>
                </div>
                <div class="layui-input-inline">
                    <select >
                        <option value="">请选择市</option>
                        <option value="杭州">杭州</option>
                        <option value="宁波">宁波</option>
                        <option value="温州">温州</option>
                        <option value="温州">台州</option>
                        <option value="温州">绍兴</option>
                    </select>
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">工作经验</label>
                <div class="layui-input-inline">
                    <select>
                        <option value="">不限</option>
                        <option value="1">应届毕业生</option>
                        <option value="2">3年及以下</option>
                        <option value="3">3-5年</option>
                        <option value="4">5-10年</option>
                        <option value="5">10年以上</option>
                        <option value="6">不要求</option>
                    </select>
                </div>
                <label class="layui-form-label">学历级别</label>
                <div class="layui-input-inline">
                    <select>
                        <option value="">不限</option>
                        <option value="1">大专</option>
                        <option value="2">本科</option>
                        <option value="3">硕士</option>
                        <option value="4">博士</option>
                        <option value="5">不要求</option>
                    </select>
                </div>
                <label class="layui-form-label">期望月薪</label>
                <div class="layui-input-inline">
                    <select>
                        <option value="">不限</option>
                        <option value="1">2k以下</option>
                        <option value="2">2k-5k</option>
                        <option value="3">5k-10k</option>
                        <option value="4">10k-15k</option>
                        <option value="5">15k-25k</option>
                        <option value="6">25k-50k</option>
                        <option value="6">50k以上</option>
                    </select>
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">创建日期</label>
                <div class="layui-input-inline">
                    <input type="text" name="date" id="date" lay-verify="date" placeholder="yyyy-mm-dd" autocomplete="off" class="layui-input" onclick="layui.laydate({elem: this})">
                </div>
                <div class="layui-form-mid">-</div>
                <div class="layui-input-inline">
                    <input type="text" name="date" id="date" lay-verify="date" placeholder="yyyy-mm-dd" autocomplete="off" class="layui-input" onclick="layui.laydate({elem: this})">
                </div>
            </div>
            <div class="layui-form-item">
                <div class="layui-input-block">
                    <button class="layui-btn">搜索</button>
                    <button type="reset" class="layui-btn layui-btn-primary">重置</button>
                </div>
            </div>
        </form>
        <!--筛选条件控制按钮-->
        <div class="btn-slide-controll-wrapper">
            <button id="btn-slide-controll" class="layui-btn layui-btn-small"><i class="layui-icon">&#xe61a;</i></button>
        </div>
        <!--简历列表-->
        <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
            <legend>简历列表</legend>
        </fieldset>
        <div class="resumeTable-wrapper">
            <table id="resumeList" class="layui-table" lay-even="" lay-skin="row">
                <colgroup>
                    <col width="60">
                </colgroup>
                <thead>
                <tr>
                    <th>序号</th>
                    <th>姓名</th>
                    <th>性别</th>
                    <th>学历</th>
                    <th>工作经验</th>
                    <th>所在城市</th>
                    <th>期望工作</th>
                    <th>期望月薪</th>
                    <th>最新更改时间</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <td>1</td>
