<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>职位信息库</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <%- include ../publicPages/css_content.ejs%>
    <link rel='stylesheet' href='/stylesheets/job/jobList.css' >
</head>
<body>
<!--头部区域-->
<%- include ../publicPages/header.ejs %>
<!--主体区域-->
<div id="main-body">
    <!--导航栏-->
    <%- include ../publicPages/nav.ejs %>
    <!--文本区域-->
    <div id="content-wrapper">
        <div class="breadcrumb-wrapper">
            <span class="layui-breadcrumb"><a href="javascript:void(0);"><cite>档案信息库</cite></a><a href="javascript:void(0);"><cite>职位信息库</cite></a></span>
        </div>
        <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
            <legend>筛选条件</legend>
        </fieldset>
        <!--筛选条件-->
        <div class="filter-wrapper-parent hide">
            <form class="filter-wrapper layui-form">
                <input type="hidden" name="pageNo" value="1"/>
                <div class="layui-form-item">
                    <label class="layui-form-label">职位名称</label>
                    <div class="layui-input-block">
                        <input type="text" name="jobName" autocomplete="off" placeholder="请输入职位名称" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">工作地点</label>
                    <div class="layui-input-inline">
                        <select name="provinceId" lay-filter='province'>
                            <option value="0">不限</option>
                        </select>
                    </div>
                    <div class="layui-input-inline">
                        <select name="cityId" lay-filter='city'>
                            <option value="0">不限</option>
                        </select>
                    </div>
                    <div class="layui-input-inline">
                        <select name="countryId">
                            <option value="0">不限</option>
                        </select>
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">工作经验</label>
                    <div class="layui-input-inline">
                        <select name="experience">
                            <option value="0">不限</option>
                            <option value="1">应届毕业生</option>
                            <option value="2">3年及以下</option>
                            <option value="3">3-5年</option>
                            <option value="4">5-10年</option>
                            <option value="5">10年以上</option>
                            <option value="6">不要求</option>
                        </select>
                    </div>
                    <label class="layui-form-label">学历要求</label>
                    <div class="layui-input-inline">
                        <select name="education">
                            <option value="0">不限</option>
                            <option value="1">大专</option>
                            <option value="2">本科</option>
                            <option value="3">硕士</option>
                            <option value="4">博士</option>
                            <option value="5">不要求</option>
                        </select>
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">创建日期</label>
                    <div class="layui-input-inline">
                        <input type="text" name="startDate" id="date" placeholder="yyyy-mm-dd" autocomplete="off" class="layui-input" onclick="layui.laydate({elem: this})">
                    </div>
                    <div class="layui-form-mid">-</div>
                    <div class="layui-input-inline">
                        <input type="text" name="endDate" id="date"  placeholder="yyyy-mm-dd" autocomplete="off" class="layui-input" onclick="layui.laydate({elem: this})">
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">融资阶段</label>
                    <div class="layui-input-inline">
                        <select name="financingStage">
                            <option value="0">不限</option>
                            <option value="1">未融资</option>
                            <option value="2">天使轮</option>
                            <option value="3">A轮</option>
                            <option value="4">B轮</option>
                            <option value="5">C轮</option>
                            <option value="6">D轮及以上</option>
                            <option value="7">上市公司</option>
                            <option value="8">不需要融资</option>
                        </select>
                    </div>
                    <label class="layui-form-label">行业领域</label>
                    <div class="layui-input-inline">
                        <select name="industryField" lay-search>
                            <option value="0">不限</option>
                            <option value="1">移动互联网</option>
                            <option value="2">电子商务</option>
                            <option value="3">金融</option>
                            <option value="4">企业服务</option>
                            <option value="5">教育</option>
                            <option value="6">文化娱乐</option>
                            <option value="7">游戏</option>
                            <option value="8">O2O</option>
                            <option value="9">硬件</option>
                            <option value="10">社交网络</option>
                            <option value="11">旅游</option>
                            <option value="12">医疗健康</option>
                            <option value="13">生活服务</option>
                            <option value="14">信息安全</option>
                            <option value="15">社交网络</option>
                            <option value="16">数据服务</option>
                            <option value="17">广告营销</option>
                            <option value="18">分类信息</option>
                            <option value="19">招聘</option>
                            <option value="20">其他</option>
                        </select>
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">月薪</label>
                    <div class="layui-input-inline">
                        <select name="salary">
                            <option value="0">不限</option>
                            <option value="1">2k以下</option>
                            <option value="2">2k-5k</option>
                            <option value="3">5k-10k</option>
                            <option value="4">10k-15k</option>
                            <option value="5">15k-25k</option>
                            <option value="6">25k-50k</option>
                            <option value="7">50k以上</option>
                        </select>
                    </div>
                    <label class="layui-form-label">工作性质</label>
                    <div class="layui-input-inline">
                        <select name="jobNature">
                            <option value="0">不限</option>
                            <option value="1">全职</option>
                            <option value="2">兼职</option>
                            <option value="3">实习</option>
                        </select>
                    </div>
                </div>
                <div class="layui-form-item">
                    <div class="layui-input-block">
                        <button id="btn-search" class="layui-btn">搜索</button>
                        <button type="reset" class="layui-btn layui-btn-primary">重置</button>
                    </div>
                </div>
            </form>
        </div>
        <!--筛选条件控制按钮-->
        <div class="btn-slide-controll-wrapper">
            <button id="btn-slide-controll" class="layui-btn layui-btn-small"><i class="layui-icon">&#xe61a;</i></button>
        </div>
        <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
            <legend>职位列表</legend>
        </fieldset>
        <div class="jobTable-wrapper">
            <table id="jobList" class="layui-table" lay-even="" lay-skin="row">
                <colgroup>
                    <col width="60">
                </colgroup>
                <thead>
                <tr>
                    <th>序号</th>
                    <th>职位</th>
                    <th>工作地点</th>
                    <th>发布时间</th>
                    <th>公司名称</th>
                    <th>薪资</th>
                    <th>工作经验</th>
                    <th>学历要求</th>
                    <th>行业领域</th>
                    <th>融资阶段</th>
                    <th>操作</th>
                </tr>
